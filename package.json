{
  "name": "@gadugi/agentic-test",
  "version": "1.0.0",
  "description": "Intelligent multi-agent testing framework for Electron, CLI, and web applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "gadugi-test": "./dist/cli.js",
    "gadugi-smart-test": "./dist/runners/SmartUITestRunner.js"
  },
  "scripts": {
    "build": "tsc --skipLibCheck --noEmitOnError false && npm run postbuild",
    "postbuild": "grep -q '^#!/' dist/runners/SmartUITestRunner.js || (echo '#!/usr/bin/env node' | cat - dist/runners/SmartUITestRunner.js > temp_smart.js && mv temp_smart.js dist/runners/SmartUITestRunner.js) && chmod +x dist/cli.js dist/runners/SmartUITestRunner.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "start": "node dist/cli.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "testing",
    "agentic",
    "multi-agent",
    "typescript",
    "electron",
    "cli",
    "web",
    "playwright",
    "automation",
    "framework"
  ],
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.0",
    "chalk": "^5.6.0",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.3.1",
    "glob": "^10.3.0",
    "jimp": "^1.6.0",
    "js-yaml": "^4.1.0",
    "node-pty": "^1.0.0",
    "openai": "^4.20.0",
    "pidusage": "^4.0.1",
    "pixelmatch": "^7.1.0",
    "playwright": "^1.40.0",
    "socket.io-client": "^4.7.4",
    "systeminformation": "^5.27.8",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/cli-progress": "^3.11.6",
    "@types/glob": "^8.1.0",
    "@types/pidusage": "^2.0.5",
    "@types/pixelmatch": "^5.2.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.8",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rysweet/gadugi-agentic-test"
  },
  "author": "Ryan Sweet",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
